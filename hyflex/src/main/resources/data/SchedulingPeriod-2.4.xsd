  </xs:complexType>
  
  <xs:simpleType name="Min1Max12Int">
    <xs:restriction base="xs:integer">
      <xs:minInclusive value="1"/>
      <xs:maxInclusive value="12"/>    
    </xs:restriction>
  </xs:simpleType>
  
  <xs:simpleType name="Min0Max100Int">
